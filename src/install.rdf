<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:name>Polish Cookie Consent</em:name>
    <em:description>Automatically accepts cookie policy/GDPR on pages available in Polish.</em:description>
    <em:creator>Polish Filters Team</em:creator>
    <em:homepageURL>https://github.com/PolishFiltersTeam/PolishCookieConsent/tree/legacy</em:homepageURL>
    <em:id>PolishCookieConsentExt@polishannoyancefilters.netlify.com</em:id>
    <em:version>1.42.5</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>

    <em:optionsType>3</em:optionsType>
    <em:optionsURL>chrome://PCC/content/controlPanel/controlPanel.html</em:optionsURL>

    <em:iconURL>chrome://pcc/content/icons/icon48.png</em:iconURL>
    <em:icon64URL>chrome://pcc/content/icons/icon64.png</em:icon64URL>

    <em:localized>
      <Description>
        <em:locale>pl</em:locale>
        <em:name>Polska Ciasteczkowa Zgoda</em:name>
        <em:description>Automatycznie akceptuje politykę ciasteczek/RODO na stronach dostępnych w języku polskim.</em:description>
      </Description>
    </em:localized>

    <!-- Pale Moon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>28.0</em:minVersion>
        <em:maxVersion>29.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Basilisk -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>52.0</em:minVersion>
        <em:maxVersion>56.*</em:maxVersion>
        <em:basilisk>true</em:basilisk>
      </Description>
    </em:targetApplication>


  </Description>

</RDF>
