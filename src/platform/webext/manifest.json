{
  "manifest_version": 2,
  "author": "PolishFiltersTeam",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.43.5",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "browser_style": false,
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "applications": {
    "gecko": {
      "id": "PolishCookieConsentExt@polishannoyancefilters.netlify.com",
      "strict_min_version": "56.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "vAPI.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "controlPanel/controlPanel.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "tabs",
    "https://raw.githubusercontent.com/PolishFiltersTeam/PolishCookieConsent/master/src/cookieBase/PCB.txt"
  ],
  "background": {
    "scripts": [
      "vAPI.js",
      "vAPI_common.js",
      "update.js"
    ]
  },
  "short_name": "__MSG_extensionShortName__"
}
