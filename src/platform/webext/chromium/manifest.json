{
  "manifest_version": 3,
  "author": "FiltersHeroes",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "_version_",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_area": "navbar",
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
   "browser_specific_settings": {
    "gecko": {
      "id": "PolishCookieConsentExt@polishannoyancefilters.netlify.com",
      "strict_min_version": "109.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "vAPI.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "controlPanel/controlPanel.html",
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "storage",
    "tabs",
    "notifications",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "short_name": "__MSG_extensionShortName__"
}
